# Default AI Provider
# Specifies which AI provider to use by default
VITE_DEFAULT_PROVIDER=openai

# Default Keys Toggle (Security)
# Controls whether build-time environment keys are allowed as fallbacks
# when user keys are not present. For security, this defaults to false.
# Set to 'true' only in development or trusted environments.
VITE_DEFAULT_KEYS_ENABLED=false

# OpenAI Configuration
# Your OpenAI API key for AI features
# Obtain from: https://platform.openai.com/api-keys
VITE_OPENAI_API_KEY=

# OpenAI Model
# Default model to use for OpenAI requests (default: gpt-4o-mini)
VITE_OPENAI_MODEL=gpt-4o-mini

# Optional: Per-entity OpenAI API keys
# Replace {entityId} with actual entity IDs from your system
# Example: VITE_OPENAI_ENTITY_KEY_entity1=sk-...
# VITE_OPENAI_ENTITY_KEY_{entityId}=
